{
    "host": "127.0.0.1",
    "port": 3000,
    "backend": {
        "baseServerUrl": "http://localhost",
        "port": 5500,
        "userRoute" : {
            "baseUrl": "/api/users",
            "getUser": {
                "path": "/",
                "method": "GET"
            },
            "signup": {
                "path": "/signup",
                "method": "POST"
            },
            "login": {
                "path": "/login",
                "method": "POST"
            }
        },
        "placeRoute": {
            "baseUrl": "/api/places",
            "getPlaceById": {
                "path" : "/:pid",
                "method": "GET"
            },
            "getPlaceByUserId": {
                "path" : "/user/:uid",
                "method": "GET"
            },
            "createPlace": {
                "path" : "/",
                "method": "POST"
            },
            "updatePlace": {
                "path" : "/:pid",
                "method": "PATCH"
            },
            "deletePlace": {
                "path" : "/:pid",
                "method": "DELETE"
            }
        }
    }
}